<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<body>
    <%- include('partials/navbar') %>
    <h2 id="currentMessageTitle">Current Messages</h2>
    <div class="container">
        <div class="message-container">
                <% messages.forEach(message => { %>
                    <div class="messages">
                        <h3><%=message.user%></h3>
                        <p><%=message.text%></p>
                    </div>
                <% }); %>
        </div>
    </div>
</body>
</html>